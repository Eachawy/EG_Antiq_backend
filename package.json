    "swagger-ui-dist": "5.31.0",
    "terser-webpack-plugin": "5.3.16",
    "thread-loader": "4.0.4",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
